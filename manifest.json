{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "UXcelarate",
    "permissions": [
        "activeTab",
        "scripting",
        "webRequest"

    ],
    "action": {
        "default_popup": "js/index.html",
        "default_title": "UXcelarate",
        "default_icon": "icon.png"
    },
    "host_permissions": [
        "*://localhost:*/*"
      ]
}